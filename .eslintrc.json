{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "semi": [2, "always"],
    "indent": 2,
    "space-before-function-paren": [2, {
        "anonymous": "never",
        "named": "never"
    }],
    "comma-dangle": [2, "always-multiline"],
    "no-useless-constructor": 0,
    "camelcase": 0
  }
}
